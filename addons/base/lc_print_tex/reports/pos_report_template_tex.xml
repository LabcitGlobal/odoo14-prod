        	                    			<th class="text-right">Total</th>
											<th class="text-right">Costo</th>
											<th class="text-right">T.Cos</th>
            	            			</tr>
                	    			</thead>
                    				<tbody>
										<t t-set="total_costo" t-value="0"/>
                        				<tr t-foreach="o.lines" t-as="line">
											<t t-set="total_costo" t-value="total_costo+(line.qty*(((line['price_unit']-line['purchase_price'])*0.7)+line['purchase_price']))"/>
                        					<td class="text-right">
                                				<t t-if="o.state != 'cancel'">
                                    				<span t-field="line.qty"/>
                                				</t>
	                            			</td>
    	                        			<td><span t-field="line.product_id"/>
        	                    				<t t-if="line.line_note">
            	                					[<strong>Nota:<span t-field="line.line_note"/></strong>]
                	            				</t>
                    	        			</td>
                        	    			<td class="text-right">
                            					<span t-esc="'%.2f'%line['purchase_price']" />
                            				</td>
                        	    			<td class="text-right">
                            					<span t-esc="'%.2f'%line['price_unit']" />
                            				</td>
                            				<td class="text-right">
                            					<span t-esc="'%.2f'%(line.price_subtotal)" />                                			
	                            			</td>
                        	    			<td class="text-right">
                            					<span t-esc="'%.2f'%(((line['price_unit']-line['purchase_price'])*0.7)+line['purchase_price'])" />
                            				</td>
                            				<td class="text-right">
                            					<span t-esc="'%.2f'%(line.qty*(((line['price_unit']-line['purchase_price'])*0.7)+line['purchase_price']))" />                                			
	                            			</td>
    	                    			</tr>
        	            			</tbody>                    			
            	    			</table>
                				<div class="table table-sm o_main_table">
                    				<div class="col-xs-12 pull-right">
                        				<table class="table table-condensed">
                            				<tr class="border-black">
                                				<td><strong>IVA</strong></td>
                                				<td class="text-right">
                                    				<strong t-esc="o.amount_tax"/>
                                				</td>
	                            			</tr>
    	                        			<tr>
        	                        			<td><strong>Total</strong></td>
            	                    			<td class="text-right">                                				
                	                    			<!--<strong t-esc="int(o.amount_total)"/>-->
                    	                			<strong t-esc="'%.2f'%(o.amount_total)"/>                                    			
                        	        			</td>
            	                    			<td class="text-right">                                				
                	                    			<!--<strong t-esc="int(o.amount_total)"/>-->
                    	                			<strong t-esc="'%.2f'%(total_costo)"/>                                    			
                        	        			</td>
                            				</tr>                            				
                        				</table>
                        				<t t-if="o.note">
                        					<p><strong>NOTA:</strong> <strong t-esc="o.note"/></p>
										</t>
                    				</div>
                				</div>                				
                			</div>
                			<div class="footer">
        						<div class="text-center">        
						            <ul class="list-inline">
                						<li>Página: <span class="page"/>/<span class="topage"/></li>
            						</ul>
        						</div>
    						</div>                			
                			</div> 
                		</div>               		
            		<!-- </t> -->
        		</t>
    		</t>
		</template>	

    </data>
</odoo>

