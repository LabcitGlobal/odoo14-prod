<odoo>
  <data>  
    <record model="ir.ui.view" id="view_lccash_payment_form">      
      <field name="model">lccash.payment</field>
      <field name="inherit_id" ref="lccash.lccash_payment_form"/>
      <field name="arch" type="xml">      		
      		<xpath expr="//field[@name='note']" position="after">            	
          	<field name="delivery_id"/>
          	<field name="packer_id"/>
          </xpath>
      </field>
    </record>    
  </data>
</odoo>