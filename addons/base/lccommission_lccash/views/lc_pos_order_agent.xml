<odoo>
  <data>  
    <record model="ir.ui.view" id="view_pos_order_agent_tree">      
      <field name="model">pos.order</field>
      <field name="inherit_id" ref="point_of_sale.view_pos_order_tree"/>
      <field name="arch" type="xml">      		
      		<xpath expr="//field[@name='state']" position="after">            	
            	<field name="delivery_id" optional="hide"/>
            	<field name="packer_id" optional="hide"/>              
          </xpath>          
      </field>
    </record>
    
    <record model="ir.ui.view" id="lc_posorder_shipping_form_view">      
      <field name="model">pos.order</field>
      <field name="inherit_id" ref="point_of_sale.view_pos_pos_form"/>
      <field name="arch" type="xml">
      	<xpath expr="//page[@name='notes']" position="after">          
            <page string="Agentes" name="Files">
              <group string="Responsables">
                <field name="delivery_id"/>
                <field name="packer_id"/>
              </group>
            </page>
          </xpath>
      </field>
    </record>    
  </data>
</odoo>