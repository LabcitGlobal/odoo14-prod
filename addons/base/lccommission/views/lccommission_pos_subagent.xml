<odoo>
  <data>

    <record id="lccommission_possubagent_view_tree" model="ir.ui.view">
      <field name="name">lccommission_possubagent.tree</field>
      <field name="model">lccommission.possubagent</field>
      <field name="arch" type="xml">
          <tree>
              <field name="name"/>
              <field name="agent_id"/>
              <field name="note"/>
          </tree>
      </field>
    </record>

    <record id="lccommission_possubagent_view_form" model="ir.ui.view">
      <field name="name">lccommission_possubagent.form</field>
      <field name="model">lccommission.possubagent</field>
      <field name="arch" type="xml">
      <form string="Asignacion de Orden y Agente">      	
      	<sheet>
      		<group>
              <field name="name"/>              
              <field name="agent_id"/>
              <field name="note"/>              
              <field name="active"/>             
          </group>          	          	
      	</sheet>
      </form>
      </field>
    </record>

    <record id='lccommission_possubagent_action' model='ir.actions.act_window'>
      <field name="name">Pedidos por Sub Agente</field>
      <field name="res_model">lccommission.possubagent</field>      
      <field name="view_mode">tree,form</field>
    </record>

    <menuitem id="lccommission_possubagent_menu" parent="lccommission.pos_menu" action="lccommission_possubagent_action" groups="group_show_subagent" sequence="13"/>

  </data>
</odoo>