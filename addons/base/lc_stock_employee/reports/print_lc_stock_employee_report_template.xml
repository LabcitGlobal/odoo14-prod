<odoo>
    <data>
        <template id="print_lc_stock_employee_report_template">
            <t t-call="web.html_container">
                <t t-call="web.external_layout">                    
                    <h2 class="text-center">REPORTE DE STOCK</h2>
                    <h3 class="text-center"><span t-esc="form.get('employee')"/></h3>                                        
                    <table class="table table-sm">
                        <thead>
                            <tr>                                                                                  
                                <th class="text-center">
                                    <strong>ID</strong>
                                </th>                                
                                <th class="text-center">
                                    <strong>Almacen</strong>
                                </th>
                                <th class="text-center">
                                    <strong>Codigo</strong>
                                </th>
                                <th class="text-center">
                                    <strong>Producto</strong>
                                </th>
                                <th class="text-center">
                                    <strong>Cantidad</strong>
                                </th>                                
                            </tr>
                        </thead>
                        <tbody>
                            <tr t-foreach="form.get('stock')" t-as="l">
                                <td>
                                    <span t-esc="l.get('product_id')"/>
                                </td>                                
                                <td>
                                    <span t-esc="l.get('location')"/>
                                </td>
                                <td>
                                    <span t-esc="l.get('default_code')"/>
                                </td>
                                <td>
                                    <span t-esc="l.get('product_name')"/>
                                </td>
                                <td class="text-right">
                                    <span t-esc="l.get('quantity')"/>
                                </td>                                
                            </tr>
                        </tbody>                        
                    </table>
                    <div class="footer">
        				<div class="text-center" style="border-top: 1px solid black;">        
							<ul class="list-inline">
                				<li>Página: <span class="page"/>/<span class="topage"/></li>
                				<li>Teléfonos: <span t-field="res_company.phone"/><t t-if="res_company.website"> - Web: <span t-field="res_company.website"/></t></li>
            				</ul>
        				</div>
    				</div>
                </t>
            </t>
        </template>
    </data>
</odoo>