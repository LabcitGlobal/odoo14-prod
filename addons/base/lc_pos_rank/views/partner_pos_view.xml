<odoo>
  <data>

    <record id="lc_partner_pos_view_tree" model="ir.ui.view">
      <field name="name">lc.partner.pos.view.tree</field>
      <field name="model">lc.partner.pos.view</field>
      <field name="arch" type="xml">
          <tree>
              <field name="name"/>              
              <field name="orders" groups="lccash.group_manager"/>
              <field name="total" groups="lccash.group_manager"/>
              <field name="margin" groups="base.user_admin"/>              
          </tree>
      </field>
    </record>

    <record id="lc_partner_pos_view_form" model="ir.ui.view">
      <field name="name">lc.partner.pos.view.form</field>
      <field name="model">lc.partner.pos.view</field>
      <field name="arch" type="xml">
      <form string="Partner Rank Detail">      	
      	<sheet>
      		<group>
              <field name="name"/>              
              <field name="orders" groups="lccash.group_manager"/>
              <field name="total" groups="lccash.group_manager"/>
              <field name="margin" groups="base.user_admin"/>              
          </group>          	          	
      	</sheet>
      </form>
      </field>
    </record>

    <record id='lc_partner_pos_view_action' model='ir.actions.act_window'>
      <field name="name">Partner Rank</field>
      <field name="res_model">lc.partner.pos.view</field>      
      <field name="view_mode">tree,form</field>
    </record>

  </data>
  <menuitem id="lc_rank_partner_pos_act_menu" parent="lc_pos_rank.menu_lc_pos_order_partner_rank" name="List Partner Rank" action="lc_partner_pos_view_action" sequence="11"/>
</odoo>