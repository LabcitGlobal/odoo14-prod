<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
    <record id="paperformat_portrait" model="report.paperformat">
        <field name="name">Custom</field>
        <field name="default" eval="True" />
        <field name="format">custom</field>
        <field name="page_height">210</field>
        <field name="page_width">140</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">30</field>
        <field name="margin_bottom">12</field>
        <field name="margin_left">4</field>
        <field name="margin_right">4</field>
        <field name="header_line" eval="False" />
        <field name="header_spacing">32</field>
        <field name="dpi">100</field>
    </record>    
    <report
        id="portrait_pos_imgbg_report"
        string="Imprimir (Vertical, Fondo, Imagen, Precio)"
        model="pos.order"
        report_type="qweb-pdf"
        name="lc_print_pos_images.pos_report_background_port_img"
        file="lc_print_pos_images.pos_print_report_background"
        paperformat="paperformat_portrait"
        />
    
    <report
        id="portrait_pos_bg_report"
        string="Imprimir (Vertical, Fondo, Precio)"
        model="pos.order"
        report_type="qweb-pdf"
        name="lc_print_pos_images.pos_report_background_port"
        file="lc_print_pos_images.pos_print_report_background"
        paperformat="paperformat_portrait"
        />

    <record id="paperformat_landscape" model="report.paperformat">
        <field name="name">Letter</field>
        <field name="default" eval="True" />
        <field name="format">custom</field>
        <field name="page_height">210</field>
        <field name="page_width">140</field>        
        <field name="orientation">Landscape</field>
        <field name="margin_top">25</field>
        <field name="margin_bottom">12</field>
        <field name="margin_left">10</field>
        <field name="margin_right">10</field>
        <field name="header_line" eval="False" />
        <field name="header_spacing">32</field>
        <field name="dpi">100</field>
    </record> 
    <report
        id="landscape_pos_imgbg_report"
        string="Imprimir (Horizontal, Fondo, Imagen, Precio)"
        model="pos.order"
        report_type="qweb-pdf"
        name="lc_print_pos_images.pos_report_background_land_img"
        file="lc_print_pos_images.pos_print_report_background"
        paperformat="paperformat_landscape"
        />
    <report
        id="landscape_pos_bg_report"
        string="Imprimir (Horizontal, Fondo, Precio)"
        model="pos.order"
        report_type="qweb-pdf"
        name="lc_print_pos_images.pos_report_background_land"
        file="lc_print_pos_images.pos_print_report_background"
        paperformat="paperformat_landscape"
        />
        
        
    <report
        id="portrait_pos_imgbg_report_noprecio"
        string="Imprimir (Vertical, Fondo, Imagen)"
        model="pos.order"
        report_type="qweb-pdf"
        name="lc_print_pos_images.pos_report_background_port_img_noprecio"
        file="lc_print_pos_images.pos_print_report_background"
        paperformat="paperformat_portrait"
        />
    
    <report
        id="portrait_pos_bg_report_noprecio"
        string="Imprimir (Vertical, Fondo)"
        model="pos.order"
        report_type="qweb-pdf"
        name="lc_print_pos_images.pos_report_background_port_noprecio"
        file="lc_print_pos_images.pos_print_report_background"
        paperformat="paperformat_portrait"
        />
    <report
        id="landscape_pos_imgbg_report_noprecio"
        string="Imprimir (Horizontal, Fondo, Imagen)"
        model="pos.order"
        report_type="qweb-pdf"
        name="lc_print_pos_images.pos_report_background_land_img_noprecio"
        file="lc_print_pos_images.pos_print_report_background"
        paperformat="paperformat_landscape"
        />
    <report
        id="landscape_pos_bg_report_noprecio"
        string="Imprimir (Horizontal, Fondo)"
        model="pos.order"
        report_type="qweb-pdf"
        name="lc_print_pos_images.pos_report_background_land_noprecio"
        file="lc_print_pos_images.pos_print_report_background"
        paperformat="paperformat_landscape"
        />


    </data>      
</odoo>